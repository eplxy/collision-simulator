<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox prefHeight="216.0" prefWidth="227.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label text="Mass" />
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <TextField fx:id="massTxtField" prefHeight="25.0" prefWidth="85.0" />
            <Label text="kg" />
         </children>
      </HBox>
      <Label text="Position" />
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <TextField fx:id="posYTxtField" prefHeight="25.0" prefWidth="38.0" />
            <Label text="x" />
            <TextField fx:id="posXTxtField" prefHeight="25.0" prefWidth="39.0" />
            <Label text="y" />
         </children>
      </HBox>
      <Label text="Speed" />
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <TextField fx:id="speedTxtField" prefHeight="25.0" prefWidth="85.0" />
            <Label text="m/s" />
         </children>
      </HBox>
      <Label text="Direction" />
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <TextField fx:id="speedTxtField1" prefHeight="25.0" prefWidth="85.0" />
            <Label text="degreees" />
         </children>
      </HBox>
      <Button fx:id="btnEnter" mnemonicParsing="false" text="Enter" />
   </children>
</VBox>
